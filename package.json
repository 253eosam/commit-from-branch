{
  "name": "@253eosam/commit-from-branch",
  "version": "0.1.4",
  "description": "Flexible commit message templating from branch name (ticket/segments) for Husky prepare-commit-msg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/253eosam/commit-from-branch.git"
  },
  "bugs": {
    "url": "https://github.com/253eosam/commit-from-branch/issues"
  },
  "homepage": "https://github.com/253eosam/commit-from-branch#readme",
  "type": "module",
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./init": {
      "types": "./dist/init.d.ts",
      "import": "./dist/init.js",
      "require": "./dist/init.cjs"
    }
  },
  "bin": {
    "cfb": "bin/cfb.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup src/core.ts src/cli.ts src/init.ts --dts --format cjs,esm --out-dir dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
